  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "android-production": "ENVFILE=.env.production react-native run-android --variant=release",
    "android-apk": "cd android && ENVFILE=.env.production ./gradlew assembleRelease && cd..",
    "release-apk": "cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd..",
    "ios": "ENVFILE=.env react-native run-ios --simulator=\"iPhone X\"",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "codepush-info-android": "code-push deployment ls -k HiSoftvn/Avixo-Patient-Android",
    "codepush-info-ios": "code-push deployment ls -k HiSoftvn/Avixo-Patient-iOS",
    "codepush-ios:staging": "appcenter codepush release-react -k -a HiSoftvn/Avixo-Patient-iOS -d Staging",
    "codepush-ios:production": "appcenter codepush promote -a HiSoftvn/Avixo-Patient-iOS -s Staging -d Production",
    "codepush-android:staging": "appcenter codepush release-react -k -a HiSoftvn/Avixo-Patient-Android -d Staging",
    "codepush-android:production": "appcenter codepush promote -a HiSoftvn/Avixo-Patient-Android -s Staging -d Production",
    "postinstall": "scripts/setup.sh",
    "ios:beta": "fastlane ios beta",
    "ios:screen-shot": "fastlane ios screenshots",
    "generate-component": "plop component",
    "generate-context": "plop context",
    "generate-screen": "plop screen"
